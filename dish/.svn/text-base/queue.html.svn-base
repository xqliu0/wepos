<div class="wp-modals ng-scope">
  <div id="clear-queue-oper-confirm-panel" class="wp-modal wp-confirm-modal ng-isolate-scope ng-hide" ng-show="wpShow" wp-show="confirm_modal.show" wp-lock-mask="confirm_modal.lock_mask" wp-confirm-info="确定入号用餐？ B-003" wp-ok-text="确定" wp-cancel-text="取消" wp-ok="confirm_modal.ok();" wp-cancel="confirm_modal.cancel();">
  <div class="wp-modal-mask" onclick="click_mask()"></div>
  <div class="wp-modal-body">
  <div id="confirm-note" class="wp-confirm-info ng-binding" ng-bind-html="wpConfirmInfo|unsafe">确定要清除今天的所有队列排号吗？</div>
  <div class="wp-confirm-actions">
  <div class="wp-btn-block wp-btn"  onclick="confirmClearQueue();" ng-show="wpOkText">
  <div class="text">
  <div class="overflow-hidden-text" ng-transclude="">
  <span class="ng-binding ng-scope">确定</span>
  </div>
  </div>
  </div>
  <div class="wp-btn-block wp-btn"  onclick="click_mask();" ng-show="wpCancelText">
  <div class="text">
  <div class="overflow-hidden-text" ng-transclude="">
  <span class="ng-binding ng-scope">取消</span></div></div></div>
  </div></div></div>
</div>
</div>

<!-- 队列列表 -->
<script type="text/html" id="store_queue_list">
<% if(queue_list.length > 0){%>
	<%for(i=0;i<queue_list.length;i++){%>
		<% if(i==0){%>
			<div id="queue_<%=queue_list[i].queue_t_id;%>" class="menu ng-binding ng-scope active" ng-repeat="table_zone in table_zones" ng-class="{active: table_zone == active_table_zone}" onclick="getQueueInfo('<%=queue_list[i].queue_t_id;%>');">
       			<%=queue_list[i].queue_t_title;%>
				<% if(queue_list[i].queue_t_giveout>0){%>
					<span class="point-number ng-binding"><%=queue_list[i].queue_t_giveout;%></span>
      			<%}%>
			</div>	
		<%} else {%> 
			<div id="queue_<%=queue_list[i].queue_t_id;%>" class="menu ng-binding ng-scope" ng-repeat="table_zone in table_zones" ng-class="{active: table_zone == active_table_zone}" onclick="getQueueInfo('<%=queue_list[i].queue_t_id;%>');">
       			<%=queue_list[i].queue_t_title;%>
				<% if(queue_list[i].queue_t_giveout>0){%>
					<span class="point-number ng-binding"><%=queue_list[i].queue_t_giveout;%></span>
      			<%}%>
      		</div>
		<%}%>   
	<%}%>
	<div id="history_queue_nemu" class="menu" onclick="get_history()" ng-class="{active: show_all_tables }">
          	历史
    </div>
   
 <%}%> 
</script>


<!-- 队列列表 -->
<script type="text/html" id="oper_store_queue_list">
<% if(queue_list.length > 0){%>
	<%for(i=0;i<queue_list.length;i++){%>
		<% if(i==0){%>
			<div id="oper-queue-t-<%=queue_list[i].queue_t_id;%>" class="wp-btn-block wp-btn ng-scope active"  ng-repeat="queue_setting in queue_settings" 
onclick="choose_queue('<%=queue_list[i].queue_t_id;%>')" ng-class="{'active': new_guest_queue.queue_setting_id == queue_setting.id}">
		<%} else {%>
			<div id="oper-queue-t-<%=queue_list[i].queue_t_id;%>" class="wp-btn-block wp-btn ng-scope"  ng-repeat="queue_setting in queue_settings" 
onclick="choose_queue('<%=queue_list[i].queue_t_id;%>')" ng-class="{'active': new_guest_queue.queue_setting_id == queue_setting.id}">
		<%}%>
			<div class="text">
					<div class="overflow-hidden-text" ng-transclude="">
						<span class="ng-binding ng-scope"><%=queue_list[i].queue_t_title;%></span>
					</div>
				</div>
			</div>  
	<%}%>  
 <%}%> 
</script>


<!-- 队列详细信息列表 -->
<script type="text/html" id="store_queue_detail_list">
<% if(queue_list.length > 0){%>
	<%for(i=0;i<queue_list.length;i++){%>
		<% if(i%2==0){%>
			<tr  class="ng-scope odd">
		<% }else{%>
			<tr  class="ng-scope">
		<% }%>
		 <td>
            <span class="guest-no ng-binding"><%=queue_list[i].queue_code;%></span>
            <span class="guest-no ng-hide" ng-show="guest_queue.has_pre_order"><br>已预点菜</span>
          </td>
          <td class="sm-font">
            <span ng-show="guest_queue.phone" class="ng-binding"><%=queue_list[i].queue_customer_phone;%><br></span>
            <span class="ng-binding"><%=queue_list[i].queue_customer_num;%>人<br></span>
            <span ng-hide="active_history" class="ng-binding">已等候<%=queue_list[i].wait_time;%><br></span>
          </td>
          <td ng-show="active_history" class="ng-binding ng-hide">排队中</td>
          <td ng-class="{ 'binded': guest_queue.is_binded}" class="sm-font ng-binding">
            <span ng-show="guest_queue.track_from == 'FromWechat'" class="green ng-hide"><i class="fa fa-wechat"></i></span>
            收银端
            <br>
            未绑定微信
          </td>
          <td ng-show="active_history" class="ng-binding ng-hide">2015-10-15 18:23</td>
          <td ng-hide="active_history">
            <div class="queue-btn green wp-btn-block wp-btn-sm"  ng-click="voice_notify(guest_queue)" ng-class="{'disabled': !can_voice_notify(guest_queue)}"><div class="text"><div class="overflow-hidden-text" ng-transclude="">
              <i class="fa fa-volume-up green ng-scope"></i><span class="ng-scope">
              叫号
            </span></div></div></div>
            <div class="queue-btn blue wp-btn-block wp-btn-sm"  onclick="updateQueue('<%=queue_list[i].queue_id;%>','<%=queue_list[i].queue_code;%>','accept')" ng-class="{'disabled': !can_op(guest_queue)}"><div class="text"><div class="overflow-hidden-text" ng-transclude="">
              <i class="fa fa-cutlery blue ng-scope"></i><span class="ng-scope">
              就餐
            </span></div></div></div>
            <div class="queue-btn red wp-btn-block wp-btn-sm"  onclick="updateQueue('<%=queue_list[i].queue_id;%>','<%=queue_list[i].queue_code;%>','pass')" ng-class="{'disabled': !can_op(guest_queue)}"><div class="text"><div class="overflow-hidden-text" ng-transclude="">
              <i class="fa fa-sign-out red ng-scope"></i><span class="ng-scope">
              过号
            </span></div></div></div>
            <div class="queue-btn gray wp-btn-block wp-btn-sm wp-btn-block wp-btn-sm"  onclick="moreAction('<%=queue_list[i].queue_id;%>','<%=queue_list[i].queue_code;%>','pass')"><div class="text"><div class="overflow-hidden-text" ng-transclude="">
              <i class="fa fa-wechat green ng-scope ng-hide" ng-show="can_print_pre_order(guest_queue)"></i>
              <i class="fa fa-bars gray ng-scope" ng-hide="can_print_pre_order(guest_queue)"></i><span class="ng-scope">
              更多
            </span></div></div></div>
          </td>
          <td ng-show="active_history" class="ng-hide">
            <div class="queue-btn orange wp-btn-block wp-btn-sm"  ng-click="reprint(guest_queue)" ng-class="{'disabled': !can_reprint(guest_queue)}"><div class="text"><div class="overflow-hidden-text" ng-transclude="">
              <i class="fa fa-print orange ng-scope"></i><span class="ng-scope">
              小票补打
            </span></div></div></div>
          </td>
        </tr>
<%}%>
<%}%>
</script>



